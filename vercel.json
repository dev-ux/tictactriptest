{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/index.js",
      "headers": {
        "Content-Type": "text/plain"
      },
      "methods": ["POST"],
      "maxBodyLength": 52428800
    }
  ]
}
